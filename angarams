#include<stdio.h>
#include<string.h>
int main()
{
	char s1[100],s2[100];
	int j,i,n1,n2,count=0;
	char temp;
	printf("enter the first string:");
	gets(s1);
	printf("enter the second string:");
	gets(s2);
	n1=strlen(s1);
	n2=strlen(s2);
	for(i=0;i<n1-1;i++)
	{
		for(j=0;j<n1-i-1;j++)
		{
			if(s1[j]>s1[j+1])
			{
				temp=s1[j];
				s1[j]=s1[j+1];
				s1[j+1]=temp;
			}
		}
	}
	
	for(i=0;i<n2-1;i++)
	{
		for(j=0;j<n2-i-1;j++)
		{
			if(s2[j]>s2[j+1])
			{
				temp=s2[j];
				s2[j]=s2[j+1];
				s2[j+1]=temp;
			}
		}
	}

	for(i=0;i<n1;i++)
	{
		if(s1[i]==s2[i])
		{
			count++;
		}
	}
	if(count==n1)
	{
		printf("anagarams");
	}else
	{
		printf("not anagarams");
	}

}
